{
  "name": "@amonlibanio/n8n-nodes-cogfy",
  "version": "1.1.2",
  "description": "n8n community node package for Cogfy API integration",
  "keywords": [
    "n8n-community-node-package",
    "cogfy",
    "api",
    "messenger",
    "tables",
    "database",
    "automation",
    "n8n",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://github.com/amonlibanio/n8n-nodes-cogfy#readme",
  "author": {
    "name": "Amon Libanio",
    "email": "amonlibanio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amonlibanio/n8n-nodes-cogfy.git"
  },
  "bugs": {
    "url": "https://github.com/amonlibanio/n8n-nodes-cogfy/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "test": "jest",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CogfyMessengerApi.credentials.js",
      "dist/credentials/CogfyTablesApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CogfyMessenger/CogfyMessenger.node.js",
      "dist/nodes/CogfyTables/CogfyTables.node.js"
    ]
  },
  "dependencies": {
    "@amonlibanio/n8n-openapi-node": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "jest": "^30.1.3",
    "n8n-workflow": "*",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
