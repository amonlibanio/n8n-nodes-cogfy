{
	"name": "@amonlibanio/n8n-nodes-cogfy",
	"version": "1.1.15",
	"description": "n8n community node package for Cogfy API integration",
	"keywords": [
		"n8n-community-node-package",
		"cogfy",
		"api",
		"messenger",
		"tables",
		"database",
		"automation",
		"n8n",
		"workflow"
	],
	"license": "MIT",
	"homepage": "https://github.com/amonlibanio/n8n-nodes-cogfy#readme",
	"author": {
		"name": "Amon Libanio",
		"email": "amonlibanio@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/amonlibanio/n8n-nodes-cogfy.git"
	},
	"bugs": {
		"url": "https://github.com/amonlibanio/n8n-nodes-cogfy/issues"
	},
	"engines": {
		"node": ">=22",
		"pnpm": ">=10.16.1"
	},
	"packageManager": "pnpm@10.16.1",
	"scripts": {
		"build": "n8n-node build && gulp build:icons",
		"dev": "n8n-node dev",
		"test": "jest",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix"
	},
	"files": [
		"dist"
	],
	"type": "commonjs",
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/CogfyMessengerApi.credentials.js",
			"dist/credentials/CogfyTablesApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/CogfyMessenger/CogfyMessenger.node.js",
			"dist/nodes/CogfyTables/CogfyTables.node.js"
		]
	},
	"dependencies": {
		"@amonlibanio/n8n-openapi-node": "^1.0.0"
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^9.35.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.1",
		"n8n-workflow": "*",
		"openapi-types": "^12.1.3",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
